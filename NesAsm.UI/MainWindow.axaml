<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="256" d:DesignHeight="340"
        Width="768" Height="743"
        x:Class="NesAsm.UI.MainWindow"
        Title="NesAsm.UI">
    <StackPanel>
        <Menu>
            <MenuItem Header="Games">
                <MenuItem Header="Jump Man" Click="MenuItem_Click" />
                <MenuItem Header="Vertical Scrolling" Click="MenuItem_Click" />
                <MenuItem Header="Boxing Game" Click="MenuItem_Click" />
            </MenuItem>
        </Menu>
        <Label Name="FrameCounter" />
        <Image Name="Image" Width="768" Height="720" RenderOptions.BitmapInterpolationMode="None" />
    </StackPanel>
</Window>
